{% func Dropdown(items map[string]string) %}
  <nav role="navigation" class="dropdown fixed right-4 top-4">
    <span class="material-icons 3xl">settings</span>
    <ul class="right-0 top-6">
      {% for label, link := range items %}
      <a class="no-underline" href="{%s link %}"><li>{%s label %}</li></a>
      {% endfor %}
    </ul>
  </nav>
{% endfunc %}
